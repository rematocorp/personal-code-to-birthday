{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"rootDir": "./src",
		"outDir": "./demo",
		"outFile": "./demo/index.js",
		"target": "ES2022",
		"module": "system",
		"baseUrl": "./",
		"sourceMap": true,
		"noImplicitAny": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"newLine": "lf"
	},
	"exclude": ["./node_modules"]
}
